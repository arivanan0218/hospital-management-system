# Production Environment Variables for Hospital Management System
# These are used in the AWS deployment

# MCP Bridge URL (points to the AWS load balancer)
VITE_MCP_BRIDGE_URL=http://hospital-alb-1667599615.us-east-1.elb.amazonaws.com:3001

# Database URL for production (internal container networking)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/hospital_management

# API Keys (set these in your task definition or AWS Secrets Manager)
GEMINI_API_KEY=your_gemini_api_key_here
VITE_CLAUDE_API_KEY=your_claude_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GROQ_API_KEY=your_groq_api_key_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Production settings
NODE_ENV=production
PORT=3001
