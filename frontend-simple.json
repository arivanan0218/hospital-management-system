{
  "family": "hospital-frontend-simple",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::324037286635:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hospital-frontend",
      "image": "324037286635.dkr.ecr.us-east-1.amazonaws.com/hospital-frontend:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
