{
  "name": "hospital-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Hospital Management System",
  "main": "dist/index.js",
  "scripts": {
    "server:build": "tsc",
    "server:build:watch": "tsc --watch",
    "server:dev": "tsx src/server.ts",
    "server:inspect": "set DANGEROUSLY_OMIT_AUTH=true && npx @modelcontextprotocol/inspector npm run server:dev",
    "client:dev": "tsx src/client.ts",
    "client:python": "python src/client.py",
    "setup:python": "python setup.py"
  },
  "keywords": [
    "hospital",
    "management",
    "api",
    "typescript",
    "node.js"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.16.2",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^29.5.0",
    "@types/methods": "^1.1.4",
    "@types/node": "^20.0.0",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/send": "^0.17.5",
    "@types/serve-static": "^1.15.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@inquirer/prompts": "^7.8.0",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "ai": "^4.3.19",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "zod": "^3.25.76"
  }
}
